module DataTypes.Primitives.Numeric.Math.Integer {

    // An unconstrained integer, adhering to the rules of mathematics, also known
    // as a "bignum", in many languages

    function mathematical_integer_operations() {
        i:Math.Integer := 0
        i := i + 2
        i := i - 1
        i := i * 3
        i := i / 2
        i += 13
        i -= 12
        i *= 2
        i /= 2
    }
}
